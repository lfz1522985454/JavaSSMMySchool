package com.bdqn.tcmp.dto;

import com.bdqn.tcmp.util.Page;

public class ClassDTO extends Page{
	private String className;
	private int teacherId;
	private int headTeacherId;
	public String getClassName() {
		return className;
	}
	public void setClassName(String className) {
		this.className = className;
	}
	public int getTeacherId() {
		return teacherId;
	}
	public void setTeacherId(int teacherId) {
		this.teacherId = teacherId;
	}
	public int getHeadTeacherId() {
		return headTeacherId;
	}
	public void setHeadTeacherId(int headTeacherId) {
		this.headTeacherId = headTeacherId;
	}

	public String toString() {
		return "ClassDTO [className=" + className + ", teacherId=" + teacherId + ", headTeacherId=" + headTeacherId
				+ "]";
	}
	
}
