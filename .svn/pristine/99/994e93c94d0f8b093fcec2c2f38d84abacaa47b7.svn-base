<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bdqn.tcmp.mapper.ClassMapper">

	<resultMap type="Class" id="ClassList">
		<id column="classId" property="classId" />
		<result column="className" property="className" />
		<result column="teacherId" property="teacherId" />
		<result column="headTeacherId" property="headTeacherId" />
		<result column="isEnabled" property="isEnabled" />
		<result column="remark" property="remark" />
	</resultMap>

	<select id="selectClassAll" resultMap="ClassList">
		select * from class
	</select>

</mapper>