package com.bdqn.tcmp.entity;
import java.io.Serializable;
/**
 * 实体类
 * @author admin
 */
public class Workplan implements Serializable{
		/*对于日期类型需要使用的注解
			@DateTimeFormat(pattern="yyyy-MM-dd")
			@DateTimeFormat(pattern="yyyy-MM-dd HH:mm:ss")
			@JSONField(format = "yyyy-MM-dd")    解决json乱码
		*/
	/**
	 * 计划编号
	 */
	private Integer planid;
	/**
	 * 班级名称
	 */
	private String className;
	/**
	 * 班级编号
	 */
	private Integer classid;
	/**
	 * 周一
	 */
	private Integer zhouyi;
	/**
	 * 周二
	 */
	private Integer zhouer;
	/**
	 *
	 */
	private Integer zhousan;
	/**
	 *
	 */
	private Integer zhousi;
	/**
	 *
	 */
	private Integer zhouwu;
	/**
	 *
	 */
	private Integer zhouliu;
	/**
	 *
	 */
	private Integer zhouri;

	/**
	 * --无参构造方法
	 */
	public Workplan()
	{
	}
	/**
	 * --有参构造方法
	 * @param planid 计划编号
	 * @param className 班级名称
	 * @param classid 班级编号
	 * @param zhouyi 周一
	 * @param zhouer 周二
	 * @param zhousan
	 * @param zhousi
	 * @param zhouwu
	 * @param zhouliu
	 * @param zhouri
	 */
	public Workplan
	(
			Integer planid,
			String className,
			Integer classid,
			Integer zhouyi,
			Integer zhouer,
			Integer zhousan,
			Integer zhousi,
			Integer zhouwu,
			Integer zhouliu,
			Integer zhouri
	)
	{
		this.planid    = planid;
		this.className = className;
		this.classid   = classid;
		this.zhouyi    = zhouyi;
		this.zhouer    = zhouer;
		this.zhousan   = zhousan;
		this.zhousi    = zhousi;
		this.zhouwu    = zhouwu;
		this.zhouliu   = zhouliu;
		this.zhouri    = zhouri;

	}

	public Integer getPlanid() {
		return planid;
	}

	public void setPlanid(Integer planid) {
		this.planid = planid;
	}

	public String getClassName() {
		return className;
	}

	public void setClassName(String className) {
		this.className = className;
	}

	public Integer getClassid() {
		return classid;
	}

	public void setClassid(Integer classid) {
		this.classid = classid;
	}

	public Integer getZhouyi() {
		return zhouyi;
	}

	public void setZhouyi(Integer zhouyi) {
		this.zhouyi = zhouyi;
	}

	public Integer getZhouer() {
		return zhouer;
	}

	public void setZhouer(Integer zhouer) {
		this.zhouer = zhouer;
	}

	public Integer getZhousan() {
		return zhousan;
	}

	public void setZhousan(Integer zhousan) {
		this.zhousan = zhousan;
	}

	public Integer getZhousi() {
		return zhousi;
	}

	public void setZhousi(Integer zhousi) {
		this.zhousi = zhousi;
	}

	public Integer getZhouwu() {
		return zhouwu;
	}

	public void setZhouwu(Integer zhouwu) {
		this.zhouwu = zhouwu;
	}

	public Integer getZhouliu() {
		return zhouliu;
	}

	public void setZhouliu(Integer zhouliu) {
		this.zhouliu = zhouliu;
	}

	public Integer getZhouri() {
		return zhouri;
	}

	public void setZhouri(Integer zhouri) {
		this.zhouri = zhouri;
	}
}
