package com.bdqn.tcmp.entity;

import com.alibaba.fastjson.annotation.JSONField;

import java.io.Serializable;
import java.util.Date;
/**
 * 实体类
 * @author admin
 */
public class Lesson implements Serializable{
		/*对于日期类型需要使用的注解
			@DateTimeFormat(pattern="yyyy-MM-dd")
			@DateTimeFormat(pattern="yyyy-MM-dd HH:mm:ss")
			@JSONField(format = "yyyy-MM-dd")    解决json乱码
		*/
    /**
     * 课表编号
     */
    private String lessonID;
    /**
     * 日期
     */
    @JSONField(format="yyyy-MM-dd")
    private Date lessonDate;
    /**
     * 课程名称
     */
    private String subjectNameA;
    /**
     * 教室
     */
    private String roomIDA;
    /**
     *
     */
    private String classIDA;
    /**
     *
     */
    private String teacherIDA;
    /**
     *
     */
    private String subjectNameB;
    /**
     *
     */
    private String roomIDB;
    /**
     *
     */
    private String classIDB;
    /**
     *
     */
    private String teacherIDB;
    /**
     *
     */
    private String subjectNameC;
    /**
     *
     */
    private String roomIDC;
    /**
     *
     */
    private String classIDC;
    /**
     *
     */
    private String teacherIDC;
    /**
     *
     */
    private String subjectNameD;
    /**
     *
     */
    private String roomIDD;
    /**
     *
     */
    private String classIDD;
    /**
     *
     */
    private String teacherIDD;
    private String teacherName;
    private String className;

    public String getTeacherName() {
        return teacherName;
    }

    public void setTeacherName(String teacherName) {
        this.teacherName = teacherName;
    }

    public String getClassName() {
        return className;
    }

    public void setClassName(String className) {
        this.className = className;
    }

    /**
     * --无参构造方法
     */
    public Lesson()
    {
    }
    /**
     * --有参构造方法
     * @param lessonID 课表编号
     * @param lessonDate 日期
     * @param subjectNameA 课程名称
     * @param roomIDA 教室
     * @param classIDA
     * @param teacherIDA
     * @param subjectNameB
     * @param roomIDB
     * @param classIDB
     * @param teacherIDB
     * @param subjectNameC
     * @param roomIDC
     * @param classIDC
     * @param teacherIDC
     * @param subjectNameD
     * @param roomIDD
     * @param classIDD
     * @param teacherIDD
     */
    public Lesson
    (
            String lessonID,
            Date lessonDate,
            String subjectNameA,
            String roomIDA,
            String classIDA,
            String teacherIDA,
            String subjectNameB,
            String roomIDB,
            String classIDB,
            String teacherIDB,
            String subjectNameC,
            String roomIDC,
            String classIDC,
            String teacherIDC,
            String subjectNameD,
            String roomIDD,
            String classIDD,
            String teacherIDD
    )
    {
        this.lessonID     = lessonID;
        this.lessonDate   = lessonDate;
        this.subjectNameA = subjectNameA;
        this.roomIDA      = roomIDA;
        this.classIDA     = classIDA;
        this.teacherIDA   = teacherIDA;
        this.subjectNameB = subjectNameB;
        this.roomIDB      = roomIDB;
        this.classIDB     = classIDB;
        this.teacherIDB   = teacherIDB;
        this.subjectNameC = subjectNameC;
        this.roomIDC      = roomIDC;
        this.classIDC     = classIDC;
        this.teacherIDC   = teacherIDC;
        this.subjectNameD = subjectNameD;
        this.roomIDD      = roomIDD;
        this.classIDD     = classIDD;
        this.teacherIDD   = teacherIDD;

    }

    @Override
    public String toString() {
        return "Lesson{" +
                "lessonID=" + lessonID +
                ", lessonDate=" + lessonDate +
                ", subjectNameA='" + subjectNameA + '\'' +
                ", roomIDA='" + roomIDA + '\'' +
                ", classIDA=" + classIDA +
                ", teacherIDA=" + teacherIDA +
                ", subjectNameB='" + subjectNameB + '\'' +
                ", roomIDB='" + roomIDB + '\'' +
                ", classIDB=" + classIDB +
                ", teacherIDB=" + teacherIDB +
                ", subjectNameC='" + subjectNameC + '\'' +
                ", roomIDC='" + roomIDC + '\'' +
                ", classIDC=" + classIDC +
                ", teacherIDC=" + teacherIDC +
                ", subjectNameD='" + subjectNameD + '\'' +
                ", roomIDD='" + roomIDD + '\'' +
                ", classIDD=" + classIDD +
                ", teacherIDD=" + teacherIDD +
                '}';
    }

    public String getLessonID() {
        return lessonID;
    }

    public void setLessonID(String lessonID) {
        this.lessonID = lessonID;
    }

    public Date getLessonDate() {
        return lessonDate;
    }

    public void setLessonDate(Date lessonDate) {
        this.lessonDate = lessonDate;
    }

    public String getSubjectNameA() {
        return subjectNameA;
    }

    public void setSubjectNameA(String subjectNameA) {
        this.subjectNameA = subjectNameA;
    }

    public String getRoomIDA() {
        return roomIDA;
    }

    public void setRoomIDA(String roomIDA) {
        this.roomIDA = roomIDA;
    }

    public String getClassIDA() {
        return classIDA;
    }

    public void setClassIDA(String classIDA) {
        this.classIDA = classIDA;
    }

    public String getTeacherIDA() {
        return teacherIDA;
    }

    public void setTeacherIDA(String teacherIDA) {
        this.teacherIDA = teacherIDA;
    }

    public String getSubjectNameB() {
        return subjectNameB;
    }

    public void setSubjectNameB(String subjectNameB) {
        this.subjectNameB = subjectNameB;
    }

    public String getRoomIDB() {
        return roomIDB;
    }

    public void setRoomIDB(String roomIDB) {
        this.roomIDB = roomIDB;
    }

    public String getClassIDB() {
        return classIDB;
    }

    public void setClassIDB(String classIDB) {
        this.classIDB = classIDB;
    }

    public String getTeacherIDB() {
        return teacherIDB;
    }

    public void setTeacherIDB(String teacherIDB) {
        this.teacherIDB = teacherIDB;
    }

    public String getSubjectNameC() {
        return subjectNameC;
    }

    public void setSubjectNameC(String subjectNameC) {
        this.subjectNameC = subjectNameC;
    }

    public String getRoomIDC() {
        return roomIDC;
    }

    public void setRoomIDC(String roomIDC) {
        this.roomIDC = roomIDC;
    }

    public String getClassIDC() {
        return classIDC;
    }

    public void setClassIDC(String classIDC) {
        this.classIDC = classIDC;
    }

    public String getTeacherIDC() {
        return teacherIDC;
    }

    public void setTeacherIDC(String teacherIDC) {
        this.teacherIDC = teacherIDC;
    }

    public String getSubjectNameD() {
        return subjectNameD;
    }

    public void setSubjectNameD(String subjectNameD) {
        this.subjectNameD = subjectNameD;
    }

    public String getRoomIDD() {
        return roomIDD;
    }

    public void setRoomIDD(String roomIDD) {
        this.roomIDD = roomIDD;
    }

    public String getClassIDD() {
        return classIDD;
    }

    public void setClassIDD(String classIDD) {
        this.classIDD = classIDD;
    }

    public String getTeacherIDD() {
        return teacherIDD;
    }

    public void setTeacherIDD(String teacherIDD) {
        this.teacherIDD = teacherIDD;
    }
}
